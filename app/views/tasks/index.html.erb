<h1>Tasks#index</h1>
<p>Find me in app/views/tasks/index.html.erb</p>

<% @tasks.each do |task| %>

  <h3><%= task.name %></h3>


  <p><%= task.status %></p>
  <br>
  <%= link_to "More details", task_path(task.id) %>
  <br>
  <%= link_to "edit", edit_task_path(task.id) %>
  <br>
<% end %>
<br>
<%= link_to "Add task", new_task_path %>
<br>

